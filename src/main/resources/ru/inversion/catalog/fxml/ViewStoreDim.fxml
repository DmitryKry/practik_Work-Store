<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import ru.inversion.fx.form.controls.JInvTable?>
<?import ru.inversion.fx.form.controls.JInvTableColumn?>
<?import ru.inversion.fx.form.controls.JInvToolBar?>

<VBox prefHeight="400.0" prefWidth="800.0" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.inversion.catalog.ViewStoreDimController">
<children>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button mnemonicParsing="false" onAction="#load_store" text="МАГАЗИНЫ" />
            <Button mnemonicParsing="false" onAction="#load_suppliers" text="ПОСТАВЩИКИ" />
            <Button mnemonicParsing="false" onAction="#load_product" text="ТОВАРЫ" />
            <Button mnemonicParsing="false" onAction="#load_category" text="КАТЕГОРИИ НА ТОВАРЫ" />
            <Button mnemonicParsing="false" onAction="#load_professions" text="ПРОФЕССИИ" />
         </children>
      </HBox>
<JInvToolBar fx:id="toolBar" />
<JInvTable fx:id="STORE_DIM" tableMenuButtonVisible="true" VBox.vgrow="ALWAYS">
<columns>
<JInvTableColumn fx:id="NAME" fieldName="NAME" prefWidth="384.0" text="%NAME" />
<JInvTableColumn fx:id="ADDRESS" fieldName="ADDRESS" prefWidth="392.0" text="%ADDRESS" />
</columns>
<VBox.margin>
<Insets bottom="5.0" left="5.0" right="5.0" />
</VBox.margin>
</JInvTable>
</children>
</VBox>
