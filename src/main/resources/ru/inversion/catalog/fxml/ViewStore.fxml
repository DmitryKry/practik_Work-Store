<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import ru.inversion.fx.form.controls.JInvLabel?>

<BorderPane fx:id="rootPane" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.inversion.catalog.ViewStoreController">
    <center>
        <VBox prefHeight="400.0" prefWidth="800.0" spacing="5.0">
            <children>
            <SplitPane dividerPositions="0.6979949874686717" prefHeight="402.0" prefWidth="800.0">
               <items>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <JInvLabel fx:id="adressInvLabel" />
                        <JInvLabel fx:id="dateOpenStoreInvLabel" />
                        <JInvLabel fx:id="firstNameOwnerInvLabel" />
                        <JInvLabel fx:id="maiLabel" />
                        <JInvLabel fx:id="nameStoreLabel" />
                        <JInvLabel fx:id="phoneInvLabel" />
                        <JInvLabel fx:id="timeOpenInvLabel" />
                     </children></VBox>
                  <VBox alignment="CENTER" spacing="5">
                     <children>
                              <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#load_suppliers" text="%SUPPLERS" />
                              <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#load_product" text="%PRODUCTS" />
                              <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#load_category" text="%CATEGORIES" />
                     </children>
                     <opaqueInsets>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </opaqueInsets>
                     <padding>
                        <Insets left="5.0" right="5.0" />
                     </padding>
                  </VBox>
               </items>
            </SplitPane>
            </children>
         <opaqueInsets>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </opaqueInsets>
        </VBox>
    </center>
</BorderPane>
